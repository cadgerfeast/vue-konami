{
  "name": "vue-konami",
  "version": "1.0.7",
  "unpkg": "dist/vue-konami.umd.js",
  "browser": "dist/vue-konami.umd.js",
  "main": "dist/vue-konami.cjs.js",
  "scripts": {
    "build": "npm run build:umd && npm run build:cjs",
    "build:umd": "cross-env NODE_ENV=production webpack --config config/webpack.config.umd.js --progress --hide-modules",
    "build:cjs": "cross-env NODE_ENV=production webpack --config config/webpack.config.cjs.js --progress --hide-modules",
    "lint": "eslint src/**/* test/**/*",
    "test": "jest",
    "coveralls": "npm run lint && npm run test && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "vue",
    "vuejs",
    "konami",
    "vue-konami"
  ],
  "author": "Jérémie Pichon <contact@cadgerfeast.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadgerfeast/vue-konami.git"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.3",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^24.9.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.1.2"
  }
}
